{
  "name": "x-ceed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "python": "python -m uvicorn services.python.simplified_rag_service:app --reload --port 8000",
    "video-ai": "python -m uvicorn services.python.video_ai_service_enhanced:app --reload --port 8002",
    "ai-service": "python -m uvicorn services.python.ai_service:app --reload --port 8004",
    "gemini-chat": "python -m uvicorn services.python.gemini_resume_chat_service:app --reload --port 8003",
    "job-desc-service": "python -m uvicorn services.python.job_description_service:app --reload --port 8008",
    "quiz-service": "python -m uvicorn services.python.quiz_generation_service_optimized:app --reload --port 8006",
    "python-all": "start-python-services.bat",
    "python-all-ps": "powershell -ExecutionPolicy Bypass -File start-python-services.ps1",
    "dev:full": "concurrently \"npm run python\" \"npm run video-ai\" \"npm run ai-service\" \"npm run gemini-chat\" \"npm run job-desc-service\" \"npm run quiz-service\" \"npm run dev\" --names \"RAG,VIDEO-AI,AI-SERVICE,GEMINI-CHAT,JOB-DESC,QUIZ,NEXTJS\" --prefix name --prefix-colors \"cyan,magenta,yellow,blue,red,orange,green\" --kill-others-on-fail",
    "dev:check": "node preflight-check.js",
    "dev:safe": "npm run dev:check && npm run dev:full",
    "python:install": "pip install -r requirements.txt",
    "setup:python": "python -m pip install --upgrade pip && npm run python:install",
    "setup:all": "npm install && npm run setup:python",
    "start-all": "powershell -ExecutionPolicy Bypass -File start-all.ps1",
    "start-all-simple": "start-all.bat"
  },
